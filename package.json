{
  "name": "yorkie-ui",
  "version": "0.5.18",
  "description": "Yorkie UI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yorkie-team/yorkie-ui.git"
  },
  "homepage": "https://yorkie.dev/yorkie-ui",
  "keywords": [
    "design-system",
    "components",
    "ui-library",
    "component",
    "framework",
    "theming",
    "design",
    "css",
    "ui"
  ],
  "files": [
    "dist"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json",
    "./style": "./dist/styles/globals.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean && npm run style && pnpm run build:panda && npx rollup -c",
    "build:panda": "mkdir -p dist/styles && panda cssgen --outfile dist/styles/globals.css",
    "dev": "next dev",
    "clean": "rm -rf dist tsconfig.tsbuildinfo actions chains connectors query",
    "start": "next start",
    "lint": "next lint --fix && stylelint --fix \"src/**/*.{css,scss}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "gh-pages -d storybook-static",
    "style": "panda codegen",
    "icon": "node generateIcons.js"
  },
  "dependencies": {
    "@ark-ui/react": "^2.2.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "@babel/core": "^7.24.7",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.1.3",
    "@pandacss/dev": "^0.36.1",
    "@park-ui/panda-preset": "^0.36.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-a11y": "^8.0.0",
    "@storybook/addon-docs": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/addon-themes": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.1.11",
    "@storybook/test": "^8.0.0",
    "@storybook/test-runner": "^0.17.0",
    "@svgr/rollup": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@types/mdx": "^2.0.11",
    "@types/node": "^20",
    "@types/react": "^18.2.68",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^7.2.0",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "gh-pages": "^6.1.1",
    "lint-staged": "^15.2.2",
    "next": "14.1.0",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "npm": "^10.5.0",
    "panda": "^0.6.5",
    "pnpm": "^8.15.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "rollup": "^4.18.0",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "storybook": "^8.0.0",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "ts-loader": "^9.5.1",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.2.0",
    "url-loader": "^4.1.1",
    "webpack-merge": "^5.10.0"
  },
  "lint-staged": {
    "*/**/*.{js,mjs,cjs,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --parser typescript --write"
    ]
  }
}
